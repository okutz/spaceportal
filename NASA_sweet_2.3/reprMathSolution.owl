  </owl:Class>

  <owl:Class rdf:about="#VisualSolution">
    <rdfs:subClassOf rdf:resource="#Solution"/>
    <rdfs:subClassOf rdf:resource="#Approximation"/>
  </owl:Class>


</rdf:RDF>
