<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY math "http://sweet.jpl.nasa.gov/2.3/reprMath.owl">
  <!ENTITY res "http://sweet.jpl.nasa.gov/2.3/humanResearch.owl">
  <!ENTITY sol "http://sweet.jpl.nasa.gov/2.3/reprMathSolution.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF 
   xml:base = "&sol;"
   xmlns:math = "&math;"
   xmlns:res = "&res;"
   xmlns:sol = "&sol;"
   xmlns:owl = "&owl;"
   xmlns:rdf = "&rdf;"
   xmlns:rdfs = "&rdfs;"
   xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&math;"/>
     <owl:imports rdf:resource="&res;"/>
  </owl:Ontology>

<!-- Classes -->
  <owl:Class rdf:about="#Algorithm">
    <rdfs:subClassOf rdf:resource="#Solution"/>
  </owl:Class>

  <owl:Class rdf:about="#AlgebraicSolution">
    <rdfs:subClassOf rdf:resource="#AnalyticalSolution"/>
  </owl:Class>

  <owl:Class rdf:about="#AnalyticalSolution">
    <rdfs:subClassOf rdf:resource="#Solution"/>
  </owl:Class>

  <owl:Class rdf:about="#Approximation">
    <rdfs:subClassOf rdf:resource="#Solution"/>
  </owl:Class>

  <owl:Class rdf:about="#BreadthFirstSearch">
    <rdfs:subClassOf rdf:resource="#Search"/>
  </owl:Class>

  <owl:Class rdf:about="#DepthFirstSearch">
    <rdfs:subClassOf rdf:resource="#Search"/>
  </owl:Class>

  <owl:Class rdf:about="#DivideAndConquer">
    <rdfs:subClassOf rdf:resource="#Algorithm"/>
  </owl:Class>

  <owl:Class rdf:about="#DynamicProgramming">
    <rdfs:subClassOf rdf:resource="#Solution"/>
  </owl:Class>

  <owl:Class rdf:about="#ExhaustiveSearch">
    <rdfs:subClassOf rdf:resource="#Search"/>
  </owl:Class>

  <owl:Class rdf:about="#Expansion">
