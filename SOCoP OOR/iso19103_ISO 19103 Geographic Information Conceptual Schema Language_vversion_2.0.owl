<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:iso19103="http://loki.cae.drexel.edu/~wbs/ontology/2004/09/iso-19103#"
    xmlns="http://loki.cae.drexel.edu/~wbs/ontology/2004/09/iso-19103#"
	xml:base="http://loki.cae.drexel.edu/~wbs/ontology/2004/09/iso-19103">
  <owl:Ontology rdf:about="">
    <rdfs:comment> title- Ontology for Geographic Information Conceptual Schema Language -  ISO 19103.
    creator- Bora Beran, Akm Saiful Islam, Luis Bermudez, Stephane Fellah, and Michael Piasecki. description- Ontology for ISO 19103. Vocabulary terms are declared using OWL language to support RDF applications. publisher- Drexel University . date- 2004-08-27. format - text/xml. language - English</rdfs:comment>
    <owl:versionInfo>version 2.0</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="UomAngle">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasISOStandardUnit"/>
        </owl:onProperty>
        <owl:hasValue>
          <ISOStandardUnits rdf:ID="radian">
            <domainCode xml:lang="en">angle</domainCode>
            <rdfs:comment xml:lang="en">ISO standard unit for angle
</rdfs:comment>
          </ISOStandardUnits>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#UnitOfMeasure"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Any of the measuring systems commonly used to measure angles</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="RecordPair">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#AnyValuePair"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#key"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#AttributeName"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="UomLength">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#UnitOfMeasure"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Any of the measuring systems to measure the length, distance between two entities</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasISOStandardUnit"/>
        </owl:onProperty>
        <owl:hasValue>
          <ISOStandardUnits rdf:ID="meter">
            <rdfs:comment xml:lang="en">ISO standard unit for length
</rdfs:comment>
            <domainCode xml:lang="en">length</domainCode>
          </ISOStandardUnits>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="AttributeName">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#LocalName"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#TypeName"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#attributeType"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#attributeType"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#aName"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Volume">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Measure"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#UomVolume"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#uom"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">The measure of the physical space of any 3-D geometric object</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Velocity">
    <rdfs:comment xml:lang="en">The instantaneous rate of change of position with time</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#UomTime"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#time"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Measure"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#distance"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#UomLength"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#UomVelocity"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#uom"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Dictionary"/>
  <owl:Class rdf:ID="RecordSchemaDictionary">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#elements"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#RecordSchemaPair"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Dictionary"/>
  </owl:Class>
  <owl:Class rdf:ID="ValueType"/>
  <owl:Class rdf:ID="Time">
    <rdfs:comment xml:lang="en">Designation of an instant on a selected time scale</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Measure"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#uom"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#UomTime"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="KeyValuePair">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#key"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="UomArea">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue>
          <ISOStandardUnits rdf:ID="square_meter">
            <rdfs:comment xml:lang="en">ISO standard unit for area
</rdfs:comment>
            <domainCode xml:lang="en">area</domainCode>
          </ISOStandardUnits>
        </owl:hasValue>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasISOStandardUnit"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Any of the measuring systems commonly used to measure area</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#UnitOfMeasure"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Multiplicity">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#range"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Defines a multiplicity range from lower to upper</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="RecordTypePair">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#key"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#AttributeName"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ObjectValuePair"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#TypeName"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#objectTypeValue"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="KeyType"/>
  <owl:Class rdf:ID="RecordSchemaPair">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#TypeName"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#key"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#RecordType"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#objectTypeValue"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ObjectValuePair"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="UomTime">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#UnitOfMeasure"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue>
          <ISOStandardUnits rdf:ID="second">
            <domainCode xml:lang="en">time</domainCode>
            <rdfs:comment xml:lang="en">ISO standard unit for time
</rdfs:comment>
          </ISOStandardUnits>
        </owl:hasValue>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasISOStandardUnit"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Any of the systems or methods of measuring or reckoning the passage of time and/or date, (e.g.,
seconds, minutes, days, months)</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="RecordSchema">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#schemaName"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#LocalName"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#featureClassDescription"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#RecordSchemaDictionary"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#featureClassDescription"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#schemaName"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#element"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="RecordDictionary">
    <rdfs:subClassOf rdf:resource="#Dictionary"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#elements"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#AnyValuePair"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="RecordType">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#container"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#RecordTypeDictionary"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#attributeTypes"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#attributeTypes"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#typeName"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#TypeName"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#typeName"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ObjectValuePair">
    <rdfs:subClassOf rdf:resource="#KeyValuePair"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#objectTypeValue"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="UomVolume">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue>
          <ISOStandardUnits rdf:ID="cubic_meter">
            <rdfs:comment xml:lang="en">ISO standard unit for volume

</rdfs:comment>
            <domainCode xml:lang="en">volume</domainCode>
          </ISOStandardUnits>
        </owl:hasValue>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasISOStandardUnit"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#UnitOfMeasure"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Record">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#attributes"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#RecordDictionary"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#attributes"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#recordType"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">A Record is a structure of logically related elements, and may be used as an implementation representation for features, by keeping a list of (name, value) pairs in a dictionary.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="UnitOfMeasure">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasISOStandardUnit"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#conversionToISOstandardUnit"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Any of the systems devised to measure some physical quantity such distance or area or a system devised to measure such things as the passage of time</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#uomName"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ISOStandardUnits"/>
  <owl:Class rdf:ID="AnyValuePair">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#dataTypeValue"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#KeyValuePair"/>
  </owl:Class>
  <owl:Class rdf:ID="MultiplicityRange">
    <importantNotes rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >lower=>0 and upper>=lower</importantNotes>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#upper"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#lower"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Angle">
    <rdfs:comment xml:lang="en">The amount of rotation needed to bring one line or plane into coincidence with another, generally measured in radians or degrees</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#UomAngle"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#uom"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Measure"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="TypeName">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#LocalName"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#aName"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LocalName">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#GenericName"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="UomVelocity">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasISOStandardUnit"/>
        </owl:onProperty>
        <owl:hasValue>
          <ISOStandardUnits rdf:ID="meters_per_second">
            <domainCode xml:lang="en">velocity</domainCode>
            <rdfs:comment xml:lang="en">ISO standard unit for velocity
</rdfs:comment>
          </ISOStandardUnits>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Any of the measuring systems commonly used to measure velocity</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#UnitOfMeasure"/>
  </owl:Class>
  <owl:Class rdf:ID="UomScale">
    <rdfs:subClassOf rdf:resource="#UnitOfMeasure"/>
    <rdfs:comment xml:lang="en">Any of the measuring systems commonly used to measure scale, or the ratio between quantities with the same unit</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue>
          <ISOStandardUnits rdf:ID="meters_per_meter">
            <rdfs:comment xml:lang="en">ISO standard unit for scale
</rdfs:comment>
            <domainCode xml:lang="en">scale</domainCode>
          </ISOStandardUnits>
        </owl:hasValue>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasISOStandardUnit"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Measure">
    <rdfs:comment xml:lang="en">The result from performing the act or process of ascertaining the extent, dimensions, or quantity of some entity</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#uom"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#value"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Vector">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
        <owl:onProperty>
